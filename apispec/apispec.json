{
  "openapi": "3.0.0",
  "info": {
    "title": "Multi-Tenant System API",
    "version": "1.0.0",
    "description": "API specification for multi-tenant management system"
  },
  "servers": [
    {
      "url": "{{base_url}}",
      "description": "Base server"
    }
  ],
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string"
          },
          "email": {
            "type": "string",
            "format": "email"
          },
          "tenant_id": {
            "type": "string",
            "format": "uuid",
            "nullable": true
          },
          "is_active": {
            "type": "boolean"
          }
        }
      },
      "Tenant": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string"
          },
          "subdomain": {
            "type": "string"
          },
          "entity_name": {
            "type": "string"
          },
          "is_active": {
            "type": "boolean"
          }
        }
      },
      "SuccessResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "message": {
            "type": "string"
          },
          "data": {
            "type": "object"
          }
        }
      },
      "ErrorResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "message": {
            "type": "string"
          },
          "errors": {
            "type": "object"
          }
        }
      }
    }
  },
  "paths": {
    "/auth/register": {
      "post": {
        "tags": ["Authentication"],
        "summary": "Register new tenant",
        "description": "Register a new tenant with owner user",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["name", "email", "password", "password_confirmation", "entity_name"],
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "Arta"
                  },
                  "email": {
                    "type": "string",
                    "format": "email",
                    "example": "ar.pamuda@gmail.com"
                  },
                  "password": {
                    "type": "string",
                    "example": "password25"
                  },
                  "password_confirmation": {
                    "type": "string",
                    "example": "password25"
                  },
                  "entity_name": {
                    "type": "string",
                    "example": "PT Pamuda Indonesia"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Registration successful",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SuccessResponse"
                }
              }
            }
          },
          "400": {
            "description": "Validation error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/auth/login": {
      "post": {
        "tags": ["Authentication"],
        "summary": "Login",
        "description": "Login for all user types (superadmin, landlord, tenant)",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["email", "password"],
                "properties": {
                  "email": {
                    "type": "string",
                    "format": "email",
                    "example": "superadmin@mail.com"
                  },
                  "password": {
                    "type": "string",
                    "example": "password"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Login successful",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SuccessResponse"
                }
              }
            }
          },
          "401": {
            "description": "Invalid credentials"
          }
        }
      }
    },
    "/auth/me": {
      "get": {
        "tags": ["Authentication"],
        "summary": "Get profile",
        "description": "Get current logged in user profile",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Profile retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SuccessResponse"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/auth/logout": {
      "post": {
        "tags": ["Authentication"],
        "summary": "Logout",
        "description": "Logout and revoke access token",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Logout successful"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/api/tenant": {
      "get": {
        "tags": ["Tenant"],
        "summary": "Get tenant info",
        "description": "Get current tenant information",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Tenant info retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SuccessResponse"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/api/tenant/users": {
      "get": {
        "tags": ["Tenant"],
        "summary": "Get tenant users",
        "description": "Get list of users in current tenant",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Users retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SuccessResponse"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/landlord/users/": {
      "get": {
        "tags": ["Landlord - Users"],
        "summary": "List users",
        "description": "Get list of all users",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Users retrieved successfully"
          }
        }
      }
    },
    "/landlord/users": {
      "post": {
        "tags": ["Landlord - Users"],
        "summary": "Create user",
        "description": "Create a new user",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["name", "email", "password"],
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "Customer Service"
                  },
                  "email": {
                    "type": "string",
                    "format": "email",
                    "example": "cs@mail.com"
                  },
                  "password": {
                    "type": "string",
                    "example": "passwordcs"
                  },
                  "tenant_id": {
                    "type": "string",
                    "format": "uuid",
                    "nullable": true,
                    "example": null
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "User created successfully"
          }
        }
      }
    },
    "/landlord/users/{user_id}": {
      "get": {
        "tags": ["Landlord - Users"],
        "summary": "Show user",
        "description": "Get user details by ID",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "user_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "User retrieved successfully"
          }
        }
      },
      "put": {
        "tags": ["Landlord - Users"],
        "summary": "Update user",
        "description": "Update user information",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "user_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "CS"
                  },
                  "is_active": {
                    "type": "boolean",
                    "example": false
                  },
                  "tenant_id": {
                    "type": "string",
                    "format": "uuid",
                    "nullable": true
                  },
                  "password": {
                    "type": "string",
                    "example": "100riburupiah"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "User updated successfully"
          }
        }
      }
    },
    "/landlord/users/inactivate/{user_id}": {
      "put": {
        "tags": ["Landlord - Users"],
        "summary": "Set user inactive",
        "description": "Deactivate a user",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "user_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "is_active": {
                    "type": "boolean",
                    "example": false
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "User deactivated successfully"
          }
        }
      }
    },
    "/landlord/users/activate/{user_id}": {
      "put": {
        "tags": ["Landlord - Users"],
        "summary": "Set user active",
        "description": "Activate a user",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "user_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "is_active": {
                    "type": "boolean",
                    "example": true
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "User activated successfully"
          }
        }
      }
    },
    "/landlord/tenants/": {
      "get": {
        "tags": ["Landlord - Tenants"],
        "summary": "List tenants",
        "description": "Get list of all tenants",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Tenants retrieved successfully"
          }
        }
      }
    },
    "/landlord/tenants": {
      "post": {
        "tags": ["Landlord - Tenants"],
        "summary": "Create tenant",
        "description": "Create a new tenant",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["name", "email", "password", "password_confirmation", "entity_name"],
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "Davit"
                  },
                  "email": {
                    "type": "string",
                    "format": "email",
                    "example": "davit.indoweb@gmail.com"
                  },
                  "password": {
                    "type": "string",
                    "example": "password22"
                  },
                  "password_confirmation": {
                    "type": "string",
                    "example": "password22"
                  },
                  "entity_name": {
                    "type": "string",
                    "example": "PT Ploso Klaten Wates Sejahtera"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Tenant created successfully"
          }
        }
      }
    },
    "/landlord/tenants/{tenant_id}": {
      "get": {
        "tags": ["Landlord - Tenants"],
        "summary": "Show tenant",
        "description": "Get tenant details by ID",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "tenant_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Tenant retrieved successfully"
          }
        }
      },
      "put": {
        "tags": ["Landlord - Tenants"],
        "summary": "Update tenant",
        "description": "Update tenant information",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "tenant_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "PT Wates Indah"
                  },
                  "subdomain": {
                    "type": "string",
                    "example": "ptwatesindah"
                  },
                  "is_active": {
                    "type": "boolean",
                    "example": false
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Tenant updated successfully"
          }
        }
      },
      "delete": {
        "tags": ["Landlord - Tenants"],
        "summary": "Delete tenant",
        "description": "Soft delete tenant",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "tenant_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Tenant deleted successfully"
          }
        }
      }
    },
    "/landlord/tenants/trashed": {
      "get": {
        "tags": ["Landlord - Tenants"],
        "summary": "Get trashed tenants",
        "description": "Get list of soft deleted tenants",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Trashed tenants retrieved successfully"
          }
        }
      }
    },
    "/landlord/tenants/restore/{tenant_id}": {
      "post": {
        "tags": ["Landlord - Tenants"],
        "summary": "Restore tenant",
        "description": "Restore soft deleted tenant",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "tenant_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Tenant restored successfully"
          }
        }
      }
    },
    "/landlord/tenants/force/{tenant_id}": {
      "delete": {
        "tags": ["Landlord - Tenants"],
        "summary": "Force delete tenant",
        "description": "Permanently delete tenant",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "tenant_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Tenant permanently deleted"
          }
        }
      }
    }
  },
  "tags": [
    {
      "name": "Authentication",
      "description": "Authentication endpoints for all user types"
    },
    {
      "name": "Tenant",
      "description": "Tenant information and users management"
    },
    {
      "name": "Landlord - Users",
      "description": "User management by landlord"
    },
    {
      "name": "Landlord - Tenants",
      "description": "Tenant management by landlord"
    }
  ]
}
